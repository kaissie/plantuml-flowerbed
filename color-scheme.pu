@startuml

!define base #EFFFE9
!define accent #2EC4B6

!define black #011627
!define white #FFFFF3

'Base'
skinparam {
  shadowing false
  backgroundColor white
  Arrow {
    Color     black
    FontColor black
  }
  Actor {
    FontColor black
    BackgroundColor base
    BorderColor     accent
  }
  Rectangle {
    FontColor black
    BackgroundColor white
    BorderColor     black
  }
}
'Activity'
skinparam activity {
  FontColor       black
  BackgroundColor base
  BorderColor     accent
  StartColor      black
  EndCOlor        black
  BarColor        black
  Diamond {
    FontColor       black
    BackgroundColor base
    BorderColor     accent
  }
}

'usecase'
skinparam usecase {
  FontColor       black
  BackgroundColor base
  BorderColor     accent
}
'class'
skinparam class {
  BackgroundColor base
  BorderColor     #A80036
}
skinparam package {
  BackgroundColor #FEFECE
  BorderColor     #A80036
}
skinparam stereotype {
}

'component'
skinparam component {

}

'note'
skinparam note {

}

'state'
skinparam state {

}

'sequence'
skinparam sequence {

}
@enduml
